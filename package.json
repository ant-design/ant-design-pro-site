{
  "name": "ant-design-pro-site",
  "version": "2.2.1",
  "title": "Ant Design Pro",
  "description": "site of Ant Design Pro",
  "homepage": "http://pro.ant.design/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/ant-design-pro-site"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^3.9.1",
    "axios": "^0.18.0",
    "bizcharts": "^3.2.2",
    "bizcharts-plugin-slider": "^2.0.4",
    "eslint-config-prettier": "^2.9.0",
    "lint-staged": "^7.0.5",
    "lodash-decorators": "^6.0.0",
    "lz-string": "^1.4.4",
    "parallelshell": "^3.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.0",
    "react-copy-to-clipboard": "^5.0.0",
    "react-document-title": "^2.0.3",
    "react-github-button": "^0.1.11",
    "react-intl": "^2.4.0",
    "stylelint-config-prettier": "^3.3.0"
  },
  "devDependencies": {
    "antd-tools": "^5.1.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.4.2",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bisheng": "^0.28.0",
    "bisheng-plugin-antd": "^0.16.5",
    "bisheng-plugin-description": "^0.1.4",
    "bisheng-plugin-react": "^0.6.3",
    "bisheng-plugin-toc": "^0.4.4",
    "classnames": "^2.2.5",
    "commander": "~2.17.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "eslint-tinker": "^0.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "^7.0.0",
    "gh-pages": "^1.0.0",
    "js-beautify": "^1.7.5",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "pre-commit": "^1.2.2",
    "rc-drawer-menu": "^1.1.0",
    "rc-queue-anim": "^1.3.1",
    "rc-scroll-anim": "^2.0.2",
    "rc-tween-one": "^2.2.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "shelljs": "^0.8.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "3.12.0"
  },
  "scripts": {
    "precommit": "npm run lint-staged",
    "start": "cross-env NODE_ENV=development bisheng start -c ./site/bisheng.config.js",
    "bisheng-build": "cross-env NODE_ENV=production bisheng build -c ./site/bisheng.config.js",
    "scaffold-start": "cd ./scaffold && BROWSER=none npm start",
    "scaffold-move": "node ./tool/bin/let -m",
    "scaffold-build": "cd ./scaffold && npm run build && node ../tool/bin/let -b",
    "scaffold-build:static": "node ./tool/bin/let -s && cd ./scaffold && npm run build && node ../tool/bin/let -b",
    "scaffold-install": "cd ./scaffold && node ../tool/bin/i",
    "preinstall": "npm run scaffold-install",
    "build-site": "rm -rf _site && npm run bisheng-build",
    "gh-pages": "gh-pages -d _site",
    "site": "cross-env NODE_OPTIONS=--max-old-space-size=4096 npm run build-site && npm run gh-pages",
    "components-publish": "node ./tool/bin/let -c && cd ./config/components && npm publish",
    "components-build": "node ./tool/bin/let -c && cd ./config/components",
    "clean": "rm -rf node_modules && cd ./scaffold && rm -rf node_modules",
    "lint": "npm run lint:es && npm run lint:style && npm run lint:demo",
    "lint:es": "eslint ./ --ext '.js,.jsx'",
    "lint-staged:js": "eslint --ext .js",
    "lint:style": "stylelint \"site/**/*.less\" --syntax less",
    "lint:demo": "cross-env RUN_ENV=DEMO eslint ./ --ext '.md' -c ./.eslintrc.js",
    "test": "npm run lint",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write ./site/**/**/**"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "pre-commit": [
    "precommit"
  ]
}
